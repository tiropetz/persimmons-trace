{
  "name": "next",
  "dependencies": {
    "@aspida/axios": "^1.9.1",
    "axios": "^0.26.1",
    "next": "12.0.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "styled-components": "^5.3.3"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^6.5.8",
    "@storybook/addon-interactions": "^6.5.8",
    "@storybook/addon-links": "^6.5.8",
    "@storybook/addon-viewport": "^6.4.9",
    "@storybook/react": "6.5.8",
    "@storybook/testing-react": "1.0.0-next.0",
    "@stylelint/postcss-css-in-js": "^0.37.2",
    "@swc/core": "^1.2.118",
    "@swc/jest": "^0.2.12",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.0.3",
    "@types/node": "16.11.6",
    "@types/react": "17.0.34",
    "@types/react-dom": "^17.0.11",
    "@types/styled-components": "^5.1.15",
    "@types/testing-library__jest-dom": "^5.14.2",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "babel-jest": "^27.4.4",
    "cspell": "^5.12.6",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "19.0.0",
    "eslint-config-next": "12.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-unicorn": "^38.0.1",
    "fixpack": "^4.0.0",
    "hygen": "^6.1.0",
    "jest": "^27.4.4",
    "npm-run-all": "^4.1.5",
    "open-cli": "^7.0.1",
    "openapi2aspida": "^0.19.0",
    "prettier": "^2.4.1",
    "prettier-plugin-organize-imports": "^2.3.4",
    "react-test-renderer": "^17.0.2",
    "reg-keygen-git-hash-plugin": "^0.11.1",
    "reg-suit": "^0.11.1",
    "shx": "^0.3.3",
    "storybook-addon-next-router": "^4.0.0",
    "storybook-addon-pseudo-states": "^1.14.0",
    "storycap": "^3.1.0",
    "stylelint": "13.13.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "22.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-declaration-block-no-ignored-properties": "^2.5.0",
    "stylelint-no-unsupported-browser-features": "^5.0.2",
    "stylelint-order": "^5.0.0",
    "stylelint-prettier": "^2.0.0",
    "stylelint-processor-styled-components": "^1.10.0",
    "ts-unused-exports": "^7.0.3",
    "typescript": "4.4.4"
  },
  "private": true,
  "scripts": {
    "build": "next build",
    "check-all": "run-s fix check-code test",
    "check-code": "run-p spell-check lint type-check",
    "dev": "next dev",
    "fix": "run-p fix:*",
    "fix-ts-by-eslint": "eslint \"./src/**/*.{ts,tsx}\" --fix",
    "fix-ts-by-prettier": "prettier \"./src/**/*.{ts,tsx}\" --write",
    "fix:config": "prettier \"./*.{js,json}\" \"./**/*.js\" --write",
    "fix:pack": "fixpack",
    "fix:ts": "run-s fix-ts-by-eslint fix-ts-by-prettier",
    "lint": "run-p lint:*",
    "lint:css": "stylelint \"./src/**/*.{ts,tsx}\"",
    "lint:ts": "eslint \"./src/**/*.{ts,tsx}\"",
    "spell-check": "cspell \"./src/**/*.{ts,tsx}\"",
    "start": "next start",
    "storybook": "start-storybook -p 6006 -s public",
    "test": "jest",
    "ts-unused-exports": "ts-unused-exports ./tsconfig.json --searchNamespaces",
    "type-check": "tsc",
    "compare-story": "run-s -c ss:capture reg-suit:save reg-suit:open",
    "reg-suit:open": "open-cli \"./report.html\"",
    "reg-suit:save": "reg-cli \"./.screenshot/actual\" \"./.screenshot/expected\" \"./.screenshot/diff\" -R \"./report.html\"",
    "ss:capture": "storycap http://localhost:6006 -o \".screenshot/actual\"",
    "ss:copy": "shx cp -r \"./.screenshot/actual/*\" \"./.screenshot/expected\"",
    "ss:git-add": "git add \"./.screenshot/expected\"",
    "ss:git-commit": "git commit -m \"docs: update expected screenshot@`date \"+%Y-%m-%d %H:%M:%S\"`\"",
    "ss:remove": "shx rm -rf \"./.screenshot/expected/*\"",
    "update-ss": "run-s ss:remove ss:copy ss:git-add ss:git-commit",
    "new:fc": "hygen new functional-component",
    "new:pc": "hygen new pages-component",
    "openapi": "run-p openapi:*",
    "openapi:api1": "run-s api:init api:gen",
    "api1:init": "rm -rf lib/api/sample",
    "api1:gen": "openapi2aspida -i=./../strapi/extensions/documentation/documentation/1.0.0/full_documentation.json -o=lib/api/sample"
  }
}
